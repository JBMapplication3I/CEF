<div class="panel panel-danger">
    <div class="panel-heading"><h3 class="panel-title" data-translate="ui.admin.checkout.slimCheckout.TransactionError"></h3></div>
    <div class="panel-body">
        <p ng-bind="message"></p>
        <p ng-if="result && result.Succeeded">
            <span data-translate="ui.admin.common.TransactionError.Message.Part1"></span>&nbsp;<!--
            --><span ng-bind="result.OrderID || result.OrderIDs && result.OrderIDs.join(', ')"></span>&nbsp;<!--
            --><span data-translate="ui.admin.checkout.slimCheckout.TransactionError.Message.Part2"></span>
        </p>
        <p class="text-right"
           ><button type="button" class="btn btn-default btn-secondary"
                    translate-attr="{ title: 'ui.admin.checkout.slimCheckout.Dismiss',
                                      'aria-label': 'ui.admin.checkout.slimCheckout.Dismiss' }"
                    data-translate="ui.admin.checkout.slimCheckout.Dismiss"
                    ng-click="$dismiss()"></button></p>
    </div>
</div>
