<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.net>
    <mailSettings>
      <smtp configSource="bin\mailSettings.config" />
    </mailSettings>
  </system.net>
  <system.webServer>
    <staticContent>
      <clientCache cacheControlCustom="public" cacheControlMode="UseMaxAge" cacheControlMaxAge="365.00:00:00" />
      <remove fileExtension=".svg" />
      <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
      <remove fileExtension=".woff" />
      <mimeMap fileExtension=".woff" mimeType="font/x-woff" />
      <remove fileExtension=".woff2" />
      <!--IIS requires application/octet-stream from woff2 fonts to enable CORS-->
      <mimeMap fileExtension=".woff2" mimeType="application/octet-stream" />
    </staticContent>
    <modules>
      <remove name="UrlRoutingModule-4.0" />
      <add name="UrlRoutingModule-4.0" type="System.Web.Routing.UrlRoutingModule" preCondition="managedHandler" />
    </modules>
    <httpProtocol>
      <customHeaders>
        <remove name="X-Powered-By" />
        <remove name="X-XSS-Protection" />
        <remove name="X-Frame-Options" />
        <add name="X-XSS-Protection" value="1; mode=block" />
        <add name="X-Frame-Options" value="SAMEORIGIN" />
      </customHeaders>
    </httpProtocol>
    <directoryBrowse enabled="false" />
    <rewrite>
      <rules configSource="rewrite.config" />
    </rewrite>
    <httpErrors errorMode="Detailed" />
    <urlCompression doDynamicCompression="true" />
  </system.webServer>
  <system.web>
    <machineKey validationKey="0EB599D5C3F9DC38412004A34CE97FD912C7F119" decryptionKey="A0CD57DE8982E8503C9983EA4336015A44242A9E6B1C2242" decryption="3DES" validation="SHA1" />
    <customErrors mode="Off" />
    <httpCookies httpOnlyCookies="true" requireSSL="false" domain="" />
    <globalization culture="en-US" uiCulture="en" requestEncoding="UTF-8" responseEncoding="UTF-8" fileEncoding="UTF-8" />
    <pages validateRequest="false" enableViewStateMac="true" enableEventValidation="true" viewStateEncryptionMode="Always">
      <controls />
    </pages>
    <anonymousIdentification enabled="true" cookieName=".ASPXANONYMOUS" cookieTimeout="100000" cookiePath="/" cookieRequireSSL="false" cookieSlidingExpiration="true" cookieProtection="None" domain="" />
  </system.web>
</configuration>
