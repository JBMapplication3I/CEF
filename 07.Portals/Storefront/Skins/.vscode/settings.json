{
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/*.csproj": true,
    "**/*.csproj.user": true,
    "lib/cef": true,
    "framework/telerik-kendo-ui/src/styles": true,
    "framework/telerik-kendo-ui/styles": true,
    "framework/store/jquery-plugins/**/*.css": true,
    "Scripts/thirdparty/MetroUI/css": true,
    "ADI": true,
    "ADI-Beauty": true,
    "ATHLT": true,
    "ATLAS": true,
    "BID": true,
    "BRAY": true,
    "CHEM": true,
    "Clarity-MultiBrand": true,
    "CMA": true,
    "CMMC": true,
    "GLV": true,
    "IIA-ADM": true,
    "IIA-B2B": true,
    "IIA-COB": true,
    "KEY": true,
    "MAX": true,
    "MERCK": true,
    "RDP": true,
    "ROW": true,
    "RX1": true,
    "SALES": true,
    "SCI": true,
    "SEFA": true,
    "SEFA-Shop": true,
    "SIXTY": true,
    "SPOT": true,
    "UAG": true,
    "test-*.html": true
  },
  "files.autoGuessEncoding": true,
  "html.format.maxPreserveNewLines": 1,
  "editor.detectIndentation": true,
  "[html]": {
    "editor.detectIndentation": true,
    "editor.insertSpaces": true
  },
  "[razor]": {
    "editor.detectIndentation": true,
    "editor.insertSpaces": true
  },
  "[typescript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  },
  "[javascript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  },
  "[json]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  }
}
