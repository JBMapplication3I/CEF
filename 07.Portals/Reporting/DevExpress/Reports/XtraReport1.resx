  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>USE CEF_4_5_0_BUL
DECLARE @cols AS NVARCHAR(MAX),
		@cols2 AS NVARCHAR(MAX),
		@query AS NVARCHAR(MAX),
        @query2 AS NVARCHAR(MAX),
		@query3 AS NVARCHAR(MAX);;

select @cols = STUFF((SELECT distinct ',' + REPLACE(Name,' ','')
                   from Attributes.GeneralAttribute t
                   
           FOR XML PATH(''), TYPE
           ).value('.', 'NVARCHAR(MAX)'),1,1,'')

select @cols2 = STUFF((SELECT distinct ',' + REPLACE(Name,' ','') + ' nvarchar(max)'
                   from Attributes.GeneralAttribute t
                   
           FOR XML PATH(''), TYPE
           ).value('.', 'NVARCHAR(MAX)'),1,1,'')

--PRINT @cols2


SET @query = '

		IF OBJECT_ID(''_T1'') IS NOT NULL DROP TABLE _T1

		CREATE TABLE _T1 (ProductID int, ProductName nvarchar(500), ImageName nvarchar(500), ' + @cols2 + ' )

		INSERT into _T1 EXECUTE('' 
		
		SELECT ProductID, ProductName, ImageName, ' + @cols + ' as c from 
            (
                select p.ID as ProductID, p.Name as ProductName, attrV.Value, attrG.Name,media.Name as ImageName
				FROM [Products].[Product] as p
				INNER JOIN [Products].[ProductAttribute] as pa ON pa.MasterID = p.ID
				INNER JOIN [Attributes].[AttributeValue] as attrV ON attrV.ID = pa.AttributeValueID
				INNER JOIN [Attributes].[GeneralAttribute] as attrG ON attrG.ID = attrV.AttributeID
				INNER JOIN [Products].[ProductImage] as pi on p.ID = pi.ProductID
				INNER JOIN [Media].[Library] as media on pi.LibraryID = media.ID
           ) x
		pivot 
           (
               max(Value)
               for Name in (' + @cols + ')
           ) p
		   '')

		   '

EXECUTE(@query)

SELECT * FROM _T1</value>
