  <DataSets>
    <DataSet Name="CustomerSalesCurrency">
      <Fields>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sales">
          <DataField>Sales</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>CategorySales</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select cu.[DisplayName], format(sum(os.SubtotalItems), 'c') Sales
from Contacts.[User] cu
inner join Accounts.Account aa on aa.ID = cu.AccountID
inner join Ordering.SalesOrder os on os.AccountID = aa.ID
where cu.DisplayName is not null
and os.CreatedDate &gt; @DateRange
group by cu.[DisplayName]
order by sum(os.SubtotalItems) desc</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DateRange">
            <Value>=Parameters!DateRange.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
